<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.EmployeeMapper">
    <resultMap id="employeeResultMap" type="com.example.demo.entity.Employee">
        <id property="id" column="employee_id"/>
        <result property="empName" column="employee_emp_name"/>
        <result property="empNumber" column="employee_emp_number"/>
        <result property="leaderNumber" column="employee_leader_number"/>
        <result property="lvl" column="employee_lvl"/>
    </resultMap>
</mapper>